{
  "name": "website",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "vercel-install": "yarn install && yarn vercel-submodules --all",
    "docusaurus": "docusaurus",
    "start": "concurrently -r \"npm:copy:watch\" \"npm:dev\" ",
    "start:zh": "concurrently -r \"npm:copy:watch\" \"npm:dev:zh\" ",
    "dev": "docusaurus start",
    "dev:zh": "docusaurus start --locale zh",
    "build": "docusaurus build",
    "swizzle": "docusaurus swizzle",
    "deploy": "docusaurus deploy",
    "clear": "docusaurus clear",
    "serve": "docusaurus serve",
    "write-translations": "docusaurus write-translations",
    "write-translations:zh": "docusaurus write-translations --locale zh",
    "crowdin": "crowdin",
    "crowdin:sync": "docusaurus write-translations && crowdin push",
    "crowdin:pull": "crowdin pull",
    "copy": "sh ./copy.sh",
    "copy:watch": "nodemon -V --watch modules --watch modules_i18n -e \"*\" --exec 'sh ./copy.sh'",
    "write-heading-ids": "docusaurus write-heading-ids",
    "typecheck": "tsc"
  },
  "dependencies": {
    "@crowdin/cli": "^3.9.1",
    "@docusaurus/core": "3.9.2",
    "@docusaurus/preset-classic": "3.9.2",
    "@mdx-js/react": "^3.0.0",
    "@radix-ui/react-portal": "^1.0.3",
    "classnames": "^2.3.2",
    "clsx": "^1.2.1",
    "graphql": "^16.6.0",
    "graphql-request": "^6.0.0",
    "hast-util-is-element": "1.1.0",
    "js-cookie": "^3.0.5",
    "prism-react-renderer": "^2.1.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-icons": "^4.6.0",
    "react-pdf": "^9.1.1",
    "react-use": "^17.4.0",
    "rehype-katex": "7",
    "remark-math": "6"
  },
  "devDependencies": {
    "@docusaurus/module-type-aliases": "3.9.2",
    "@docusaurus/theme-common": "3.9.2",
    "@tailwindcss/line-clamp": "^0.4.2",
    "@tsconfig/docusaurus": "^1.0.5",
    "@types/react": "^18.2.0",
    "@types/react-dom": "^18.2.0",
    "autoprefixer": "^10.4.12",
    "concurrently": "^8.1.0",
    "docusaurus-plugin-sass": "^0.2.3",
    "nodemon": "^2.0.22",
    "postcss": "^8.4.18",
    "prettier": "^2.7.1",
    "prettier-plugin-tailwindcss": "^0.1.13",
    "sass": "^1.62.1",
    "tailwindcss": "^3.2.1",
    "typescript": "^4.7.4",
    "vercel-submodules": "^1.0.10"
  },
  "browserslist": {
    "production": [
      ">0.5%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "engines": {
    "node": ">=20.0.0"
  },
  "resolutions": {
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "@types/react": "^18.2.0",
    "@types/react-dom": "^18.2.0",
    "pdfjs-dist": "^4.2.67",
    "body-parser": "^1.20.3",
    "axios": "~1.6.0",
    "wait-on": "^7.2.0"
  }
}
